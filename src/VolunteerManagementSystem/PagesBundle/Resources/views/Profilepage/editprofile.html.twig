
{% extends '::base.html.twig' %}

{% block title %}Volma - Registration{% endblock %}
{% block menu %}
{% endblock %}
{% block body %}
 <section class="container">
    <div class="login">
      <h1>Edit Profile</h1>
        {{ form_start(form) }}
        {{ form_errors(form) }}
        {{form_row(form.username)}}        
        <br><br>       
        <br><br>
        {{form_row(form.firstname)}}        
        <br>
        {{form_row(form.lastname)}}        
        <br><br>
        
        {{form_row(form.email)}}        
        <br><br>
        {{form_row(form.phoneNumber)}}        
        <br><br>
        {{form_label(form.gender)}}<br>
        {{form_widget(form.gender)}}
        <br><br>
        {{form_label(form.dateOfBirth)}}<br>
        {{form_widget(form.dateOfBirth)}}
        <br><br>
        <p align="center">
        
        <br><br>
        
        {{form_widget(form.submit)}}
         </p>
        {{ form_end(form) }}
        
    </div>
        

  </section>


{% endblock %}





